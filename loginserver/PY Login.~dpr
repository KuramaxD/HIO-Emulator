program PYLogin;

{$APPTYPE CONSOLE}

uses
  SysUtils,
  Windows;

var
  Msg: TMsg;
  bRet: LongBool;

begin
  try
    SetConsoleTitle('PY Login GB.R6.726.00');
    Writeln('[SERVER_S] Iniciando servidor.');
    repeat
      bRet := GetMessage(Msg, 0, 0, 0);
      if Integer(bRet) = -1 then begin
        Break;
      end
      else begin
        TranslateMessage(Msg);
        DispatchMessage(Msg);
      end;
    until not bRet;
  except
    on E: Exception do begin
      Writeln(E.Classname, ': ', E.Message);
  end;
end;
end.
